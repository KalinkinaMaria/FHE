<Window x:Class="FHE.Controls.EnterName"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Переименовать" Height="120" Width="300"
        MaxHeight="120" MinHeight="120"
        MaxWidth="300" MinWidth="300">
    <Grid>
        <TextBlock Text="Введите название вершины" VerticalAlignment="Top"/>
        <TextBox x:Name="nameNode" Height="25" VerticalAlignment="Center"/>
        <Button Height="25" Width="100" Content="OK" HorizontalAlignment="Right" VerticalAlignment="Bottom" Click="Button_Click"/>
    </Grid>
</Window>
